@model List<FinancialTransaction>

<h2>Search Results</h2>
<table>
    <thead>
    <tr>
        <th>Transaction ID</th>
        <th>Cardholder</th>
        <th>Merchant</th>
        <th>Date</th>
    </tr>
    </thead>
    <tbody>
    @foreach (var transaction in Model)
    {
        <tr>
            <td>@transaction.TransactionId</td>
            <td>@transaction.Card.Cardholder.AccountNumber</td>
            <td>@transaction.Merchant.MerchantCategoryCode</td>
            <td>@transaction.TransactionDate</td>
        </tr>
    }
    </tbody>
</table>